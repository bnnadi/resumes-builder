#!/usr/bin/env python3
"""
Standalone executable script for resume export.
This allows direct execution without package installation.
"""

import sys
import os
from pathlib import Path

# Add parent directory to path for direct execution
sys.path.insert(0, str(Path(__file__).parent.parent))

from resume_export.cli import main

if __name__ == '__main__':
    sys.exit(main())

